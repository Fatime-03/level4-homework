#include <iostream>
using namespace std;
int main() {
    for (int i = 1; i < 1000; i++) {
        int a = i;
        int c = 0;

        while (a > 0) {
            int b = a% 10;
            c += b * b;
            a /= 10;
        }

        if (c == i) {
            cout << i << " ";
        }
    }

    return 0;
}
