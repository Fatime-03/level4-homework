#include <iostream>
using namespace std;
int main() {
    int a = 0, b = 1, c = 0;
    cout << "1000-den kicik Fibonacci ededleri:\n";
    cout << a << " " << b << " ";
    c = a + b;
    while (c < 1000) {
        cout << c << " ";
        a = b;
        b = c;
        c = a + b;
    }
    cout << endl;
    return 0;
}
